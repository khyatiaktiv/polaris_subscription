<?xml version="1.0" ?>
<odoo>
    <record id="product_product_view_form_inherit" model="ir.ui.view">
        <field name="name">product.product.view.form.inherit</field>
        <field name="model">product.product</field>
        <field name="inherit_id" ref="product.product_normal_form_view"/>
        <field name="arch" type="xml">
             <xpath expr="//notebook" position="inside">
	             <page name="module_details" string="Module details">
					<group>
						<field name="technical_name"/>
						<field name="clone_url"/>
						<field name="document_link"/>
					</group>
	             </page>
             </xpath>
        </field>
    </record>
</odoo>
